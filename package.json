{
  "name": "driver-service",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "test": "vitest",
    "build": "tsc",
    "start": "npm run start:dev",
    "start:dev": " NODE_ENV=development sls offline --host 0.0.0.0 --stage development --reloadHandler",
    "start:test": "npm run build && NODE_ENV=test sls deploy --stage test",
    "start:prod": "npm run build && NODE_ENV=production sls deploy --stage production",
    "dev": "npm run start:dev",
    "offline": "npm run start:dev",
    "sls:dev": "npm run start:dev",
    "dev:offline": "npm run start:dev",
    "prod": "npm run start:prod",
    "deploy": "npm run build && sls deploy",
    "start:express:dev": "nodemon --config nodemon.json",
    "dev:express": "npm run start:express:dev",
    "express:dev": "npm run start:express:dev",
    "serve": "npm run start:express:dev",
    "express:offline": "npm run start:express:dev",
    "start:express:test": "dotenvx run -f .env.test -- ts-node --transpile-only ./src/server.ts",
    "start:express:prod": "dotenvx run -f .env.production -- ts-node --transpile-only ./src/server.ts",
    "deploy:express": "dotenvx run -f .env.production -- ts-node src/server.ts",
    "lint": "eslint 'src/**/*.ts' 'types/**/*.ts' 'config/**/*.ts'",
    "format": "prettier --write '**/*.{ts,js,json}'",
    "commitlint": "commitlint --edit $HUSKY_GIT_PARAMS"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.6.1",
    "@commitlint/config-conventional": "^19.6.0",
    "@types/express": "^5.0.0",
    "@types/express-winston": "^4.0.2",
    "@types/morgan": "^1.9.9",
    "@types/node": "^16.18.123",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.7",
    "@types/winston": "^2.4.4",
    "concurrently": "^9.1.2",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "express": "^4.21.2",
    "husky": "^9.1.7",
    "nodemon": "^2.0.22",
    "prettier": "^3.4.2",
    "serverless": "^4.4.18",
    "serverless-http": "^3.2.0",
    "serverless-localstack": "^1.3.1",
    "serverless-offline": "^14.4.0",
    "serverless-webpack-offline-reload": "^1.0.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.9.5",
    "vitest": "^0.34.1"
  },
  "dependencies": {
    "@nrsa/common": "latest",
    "@dotenvx/dotenvx": "^1.32.1",
    "body-parser": "^1.20.3",
    "express-winston": "^4.2.0",
    "ioredis": "^5.4.2",
    "kafkajs": "^2.2.4",
    "morgan": "^1.10.0",
    "mysql2": "^3.12.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^11.0.3",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0"
  }
}
